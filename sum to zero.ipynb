    "class Solution {\n",
    "public:\n",
    "    vector<int> sumZero(int n) {\n",
    "        vector<int> res;\n",
    "        for (int i = 1; i <= n / 2; ++1){\n",
    "            res.push_back(i);\n",
    "            res.push_back(-i);\n",
    "    }\n",
    "        if (n % 2 == 1){\n",
    "            res.push_back(0);\n",
    "        }\n",
    "        return res;\n",
    "};"
   ]
  }
